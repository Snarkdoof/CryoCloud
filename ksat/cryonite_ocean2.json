{
  "workflow": {
    "options": {
      "force": {"default": "false", "type": "bool", "help": "Force reprocessing"},
      "schema": {"default": "default.schema", "help": "JSON Schema for network poster"}
    },
    "name": "CryoniteOcean",
    "description": "CryoniteOcean SAR Wind and Wave package",
    "nodes": [{
      "module": "KSAT_netwatcher",
      "name": "NetWatcher",
      "config": "Cryonite.NetWatcher",
      "args": {
        "port": {
          "config": "port",
          "default": 1209
        }
      },
      "downstreamOf": ["entry"]
    }, {
      "module": "wind",
      "runOn": "success",
      "name": "Wind",
      "downstreamOf": ["NetWatcher"],
      "dir": "/home/njaal/git/CryoniteOcean/develop/",
      "args": {
        "src": {"output": "parent.fullpath", "type": "file", "unzip": "true"},
        "configOverride": {
          "output": "parent.configOverride",
          "default": ""
        },
        "program": "wind.sav",
        "configFile": "wind.cfg"
      }
    }, {
      "module": "sar_processor",
      "runOn": "success",
      "name": "SAR_Processor",
      "downstreamOf": ["NetWatcher"],
      "dir": "/home/njaal/git/CryoniteOcean/develop/",
      "args": {
        "src": {"output": "parent.l0fullpath", "type": "file", "unzip": "true"}
      }
    }, {
      "module": "wave",
      "runOn": "success",
      "name": "Wave",
      "downstreamOf": ["SAR_Processor"],
      "dir": "/home/njaal/git/CryoniteOcean/develop/",
      "args": {
        "src": {"output": "parent.dst", "type": "file", "unzip": "true"},
        "configOverride": {
          "output": "parent.configOverride",
          "default": ""
        },
        "program": "wave.sav",
        "configFile": "wave.cfg"
      }
    }, {
      "module": "wavemerge",
      "runOn": "success",
      "name": "WaveMerge",
      "downstreamOf": ["Wave", "Wind"],
      "dir": "/home/njaal/git/CryoniteOcean/develop/",
      "args": {
        "product": {
          "output": "Wave.dst"
        },
        "wind": {"output": "Wind.dst"},
        "program": "wave.sav",
        "configFile": "wave.cfg"
      }
    }, {
      "module": "remove",
      "ccnode": {"stat": "parent.node"},
      "downstreamOf": ["Wind", "WaveMerge"],
      "args": {
        "src": {"output": "NetWatcher.fullpath"}
      }
    }, {
      "module": "KSAT_report",
      "runOn": "success",
      "resolveOnAny": "true",
      "downstreamOf": ["WaveMerge", "Wind"],
      "args": {
        "product": {
          "output": "parent.product"
        }
      }
    }, {
      "module": "KSAT_report",
      "runOn": "error",
      "resolveOnAny": "true",
      "downstreamOf": ["WaveMerge", "Wind"],
      "args": {
        "product": {
          "output": "parent.product"
        },
        "error": {
          "output": "parent.result"
        }
      }
    }]
  }
}